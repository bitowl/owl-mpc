{
  "name": "owl-mpc",
  "version": "0.1.0",
  "description": "Get the currently playing music via mpc",
  "homepage": "https://bitowl.de",
  "author": {
    "name": "bitowl",
    "email": "bitowl@bitowl.de",
    "url": "https://bitowl.net"
  },
  "files": [
    "dashboard",
    "graphics",
    "extension.js",
    "extension"
  ],
  "keywords": [
    "mpc",
    "music",
    "nodecg-bundle"
  ],
  "nodecg": {
    "compatibleRange": "~1.1.0",
    "bundleDependencies": {
      "owl-nodecg-styles": "^0.0.0"
    },
    "dashboardPanels": [
      {
        "name": "music",
        "title": "Music",
        "width": 3,
        "file": "music.html",
        "headerColor": "#525F78"
      },
      {
        "name": "ban-current-song",
        "title": "Ban current song?",
        "width": 2,
        "file": "ban-current-song.html",
        "dialog": true,
        "dialogButtons": [
          {
            "name": "Yes",
            "type": "confirm"
          },
          {
            "name": "No",
            "type": "dismiss"
          }
        ]
      }
    ]
  },
  "dependencies": {
    "mpc-js": "^1.2.1"
  },
  "repository": "https://github.com/nodecg/nodecg.git",
  "license": "MIT"
}
